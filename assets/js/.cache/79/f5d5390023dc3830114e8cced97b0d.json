   console.log(Number(localStorage.getItem(\"Highscore\")))\r\n                //   console.log(Number(localStorage.getItem(\"Highscore\"))<currentScore)\r\n                localStorage.setItem(\"HigherScore\",currentScore)\r\n              }\r\n         }\r\n    }\r\n}\r\nconst style = {\r\n    fontSize: \"28px\" ,\r\n    fill: \"blue\",\r\n    fontFamily: \"CustomFont\",\r\n    lineSpacing: 19,\r\n    align: \"left\",\r\n    boundsAlignH: \"left\",\r\n    boundsAlignV: \"top\",\r\n    wordWrap: true,\r\n    wordWrapWidth: 300,\r\n  };\r\nexport default GameOver;"},"lineCount":null}},"error":null,"hash":"a946213a942d90b4b895feeb10815cea","cacheData":{"env":{}}}